<%= form_for(zone) do |f| %>

<div class="input-group user-form">
  <%= f.label :user_name, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :user_name, class: "form-control" %>
</div>
<br>

  <div class="input-group user-form">
  <%= f.label :email, class: "input-group-addon user-form-addon" %>
  <%= f.email_field :email, class: "form-control" %>
</div>
<br>

  <div class="input-group user-form">
  <%= f.label :name, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :name, class: "form-control" %>
  </div>
  <br>

  <div class="input-group user-form">
  <%= f.label :address, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :address, class: "form-control" %>
  </div>
  <br>

  <div class="input-group user-form">
  <%= f.label :city, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :city, class: "form-control" %>
  </div>
  <br>

  <div class="input-group user-form">
  <%= f.label :state, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :state, class: "form-control" %>
  </div>
  <br>

  <div class="input-group user-form">
  <%= f.label :zip, class: "input-group-addon user-form-addon" %>
  <%= f.text_field :zip, class: "form-control" %>
  </div>
  <br>

  <div class="input-group user-form">
  <%= f.label :password, class: "input-group-addon user-form-addon"  %>
  <%= f.password_field :password, class: "form-control" %>
  </div>
  <br>
  <div class="input-group user-form">
  <%= f.label :password_confirmation, class: "input-group-addon user-form-addon" %>
  <%= f.password_field :password_confirmation, class: "form-control" %>
  </div>
  <br>
  <br>


  <%= f.submit class: "btn btn-primary btn-lg" %>
<% end %>

</div>
