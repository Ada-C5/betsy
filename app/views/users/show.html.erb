<h1>Welcome</h1>
<h2>Account Information</h2>
<% if @user.photo_url %>
  <div>
    <img src= "<%= @user.photo_url %>" alt="@user profile pic" height="42" width="42">
  </div>
<% end %>
<div>
  <table>
    <tr>
      <td>Username</td>
      <td><%= @user.username %></td>
    </tr>
    <tr>
      <td>email</td>
      <td><%= @user.email %></td>
    </tr>
    <tr>
      <td>bio</td>
      <td><%= @user.biography %></td>
    </tr>
  </table>
</div>

<% if @user.merchant == 1 %>
  <h3>MERCHANT INFO STUFF</h3>
<% end %>
