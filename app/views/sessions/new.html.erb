<%= form_tag sessions_path, method: :post, :html => {:class => "form-signin"} do %>
<h1>Hope you remember your password!</h1>
  <%= label_tag :email %>
  <%= text_field_tag :email %>

  <%= label_tag :password  %>
  <%= password_field_tag :password  %>

  <%= submit_tag "Sign In, I guess" %>
<% end %>
